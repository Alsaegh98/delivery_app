

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>All orders</title>
  </head>
  <body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>Here's all your orders</h1>
    <a href=""></a>
    <a href="/orders/new">Add a New Order</a>

    <ul>
        <% orders.forEach((order) => { %>
        <li>
          A order from <%= order.address %> owned by <%= order.owner.number %>.
        </li>
        <a href="/orders/<%= order._id %>">View more details.</a>
        <% }) %>
      </ul>
  </body>
</html>